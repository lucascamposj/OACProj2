`timescale 1 ps/ 1 ps
module ULA_vlg_sample_tst(
	A,
	B,
	Operation,
	sampler_tx
);
input [31:0] A;
input [31:0] B;
input [3:0] Operation;
output sampler_tx;

reg sample;
time current_time;
always @(A or B or Operation)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

